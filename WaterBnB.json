    {
        "id": "8913e628e7417997",
        "type": "function",
        "z": "b136971494e5a34f",
        "g": "c33e1847e4ed69b9",
        "name": "json adapt",
        "func": "//http://stevesnoderedguide.com/node-red-functions\n\nvar nmsg = { // nouveau message \n  topic: msg.topic // on garde l'ancien topic \n};\n\nvar busy;\nif (msg.payload.piscine.oocuped === false)\n  busy = \"battery-empty\";\nelse\n  busy = \"battery-full\";\n\nvar hsc;\nif (msg.payload.piscine.hotspot === false)\n  hsc = \"Blue\";\nelse\n  hsc = \"Red\";\n\nvar wl_label = \"Lien de demande d'ouverture de \" + msg.payload.info.ident \n                + \" par \" + flow.get('clientid') + \" : Cliquez moi !\"\n\nvar base = env.get(\"WATERBNB_OPEN_URL\") || flow.get('waterbnb_open_url') || \"http://127.0.0.1:5000/open\";\nvar idu = encodeURIComponent(flow.get('clientid') || \"\");\nvar idswp = encodeURIComponent(msg.payload.info.ident || \"\");\n\nnmsg.payload = { // on modifie/adapte la payload\n\n  layer: \"swimming pools\",\n  name: msg.payload.info.ident,\n  description: msg.payload.info.user,\n  weblink: { \"name\": wl_label, \"url\": base + \"?idu=\" + idu + \"&idswp=\" + idswp },\n  radius:100,\n  unit: \"C\",\n  color: \"#000\",\n  value: msg.payload.status.temperature,\n  lat: msg.payload.location.gps.lat,\n  lon: msg.payload.location.gps.lon,\n  iconColor: hsc,\n  icon: busy\n}\n\nreturn nmsg;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 590,
        "y": 660,
        "wires": [
